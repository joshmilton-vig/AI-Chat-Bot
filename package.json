{
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm,cjs --dts --out-dir dist --clean",
    "start": "node dist/server.js",
    "check": "tsc --noEmit",
    "build:all": "npm run build && cd frontend && npm install --no-audit --no-fund && npm run build"
  },
  "dependencies": {
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/morgan": "^1.9.10"
  }
}
